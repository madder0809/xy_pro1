<include file="Public/base"/>
<script src="__ROOT__/Addons/My97DatePicker/WdatePicker.js"></script>
<div class='main'>
	<div class="details">
		<h3>实验室管理 > 实验室课表</h3>
	</div>

	<div class='header_action'>
		<span>{$type_name}</span>
		<div>
			<a href='{:U("import")}' class='modal header_btn'>导入</a>
			<a href='{:U("export")}' class='header_btn'>导出</a>
		</div>
	</div>

	<div class='main_content'>
		<div class='header_seach'>
			<form action="{:U('setStartDate')}" method="post">
			学期开始时间:<input type="text" name="value" value="{$startDate}" onClick="WdatePicker()">
			<input type="submit" value="修改">
			</form>
		</div>
	</div>
</div>
